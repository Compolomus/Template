{
  "name": "compolomus/prototype",
  "description": "Light native template engine",
  "license": "MIT",
  "keywords": [
    "template"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Koenig",
      "email": "compolom@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Compolomus\\Template\\": "src/"
    }
  },
  "require": {
    "php": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "zendframework/zend-coding-standard": "^1.0"
  },
  "suggest": {
    "ext-xdebug": "^2.6.0"
  },
  "scripts": {
    "cs-check": "phpcs",
    "cs-fix": "phpcbf",
    "test-coverage": "phpunit --coverage-clover clover.xml"
  },
  "minimum-stability": "stable"
}
